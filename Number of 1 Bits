# Method 1:

class Solution:
    def hammingWeight(self, n: int) -> int:
        weight = 0
        while n:
            n &= n - 1
            weight += 1

 # Method 2:
 
 class Solution:
    def hammingWeight(self, n: int) -> int:
       return weight
              res=0
              while n:
                  res+=n%2
                  n//=2
              return res
