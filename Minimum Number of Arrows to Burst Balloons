class Solution:
    def findMinArrowShots(self, points: List[List[int]]) -> int:
        points = sorted(points)
        p = points[0][0]
        q = points[0][1]
        ans = 0
        for i,j in points:
            if p <= i <= q or p <= j <= q:
                p = max(i,p)
                q = min(j,q)
            else:
                ans += 1
                p = i
                q = j
        return ans + 1
